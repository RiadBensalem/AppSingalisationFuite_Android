<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_hovered="true">
        <shape>
            <solid android:color="#FFFFFF"/>
            <stroke
                android:color="#000000"
                android:width="1dp">

            </stroke>
            <corners android:radius="10dp" />
            <padding
                android:left="1dp"
                android:right="1dp"
                android:top="1dp"
                android:bottom="1dp"/>

        </shape>

    </item>
    <item>
        <shape>
            <solid android:color="#FFFFFF"/>
            <stroke
                android:color="#000000"
                android:width="1dp">

            </stroke>
            <corners android:radius="10dp" />
            <padding
                android:left="5dp"
                android:right="5dp"
                android:top="5dp"
                android:bottom="5dp"/>

        </shape>

    </item>
</selector>